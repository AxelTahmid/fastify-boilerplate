{
	"name": "fastifystarter",
	"version": "1.0.0",
	"description": "This is a starter template for fastify",
	"main": "server.js",
	"type": "module",
	"engines": {
		"npm": ">=8.0.0",
		"node": ">=20.0.0"
	},
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"refresh": "bash ./database/database.sh",
		"migrate": "knex migrate:latest --knexfile=./database/knexfile.js --verbose",
		"seed": "knex seed:run --knexfile=./database/knexfile.js --verbose",
		"rollback": "knex migrate:rollback --knexfile=./database/knexfile --all --verbose",
		"make:seeder": "knex seed:make --knexfile=./database/knexfile.js",
		"make:migration": "knex migrate:make --knexfile=./database/knexfile.js",
		"key": "cd certs/ && bash generate.sh",
		"format:check": "prettier --check **/*.js .",
		"format:fix": "prettier --write **/*.js .",
		"lint:check": "eslint --ext .js .",
		"lint:fix": "eslint --ext .js . --fix",
		"update:deps": "yarn upgrade-interactive"
	},
	"author": "AxelTahmid",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.484.0",
		"@fastify/cors": "^8.5.0",
		"@fastify/formbody": "^7.4.0",
		"@fastify/helmet": "^11.1.1",
		"@fastify/jwt": "^7.2.4",
		"@fastify/multipart": "^8.0.0",
		"@fastify/rate-limit": "^9.1.0",
		"@fastify/sensible": "^5.5.0",
		"@fastify/under-pressure": "^8.3.0",
		"bcryptjs": "^2.4.3",
		"bullmq": "^5.1.1",
		"close-with-grace": "^1.2.0",
		"dotenv": "^16.3.1",
		"fastify": "^4.25.2",
		"fastify-plugin": "^4.5.1",
		"fluent-json-schema": "^4.2.1",
		"ioredis": "^5.3.2",
		"knex": "^3.1.0",
		"nodemailer": "^6.9.8",
		"ofetch": "^1.3.3",
		"pg": "^8.11.3"
	},
	"devDependencies": {
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.1",
		"nodemon": "^3.0.2",
		"npm-check": "^6.0.1",
		"pino-pretty": "^10.3.1",
		"prettier": "^3.1.1"
	},
	"packageManager": "yarn@4.0.2"
}
